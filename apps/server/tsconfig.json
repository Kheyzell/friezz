{
  "compilerOptions": {
    "module": "CommonJS",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
    //   "@common/*": ["../../common/*"],
      "@friezz/server/*": ["./src/*"],
    },
    "incremental": true,
    "skipLibCheck": true,
    "strict": true
  }
}